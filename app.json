{
  "expo": {
    "name": "VisionClara",
    "slug": "VisionClara",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.svg",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.svg",
      "resizeMode": "contain",
      "backgroundColor": "#0f172b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "statusBarStyle": "light",
      "statusBarBackgroundColor": "#0f172b"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.svg",
        "backgroundColor": "#0f172b"
      },
      "statusBar": {
        "backgroundColor": "#0f172b",
        "barStyle": "light-content"
      },
      "navigationBar": {
        "backgroundColor": "#0f172b"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/favicon.svg"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ]
    ],
    "scheme": "acme"
  }
}
